{
  "name": "aba-angular",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:local",
    "start:dev": "ng serve --configuration development",
    "start:prod": "ng serve --configuration production",
    "start:local": "ng serve --configuration local",
    "start:truelocal": "ng serve --configuration truelocal",
    "build": "npm run build:prod",
    "build:dev": "ng build --config uration development --base-href https://aba-dev.malcolmcordova.com/",
    "build:prod": "ng build --configuration production --base-href https://abatherapy.malcolmcordova.com/",
    "build:local": "ng build --configuration local --base-href /",
    "build:truelocal": "ng build --configuration truelocal --base-href /",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:dev": "ng test --browsers=ChromeHeadless --watch=true --code-coverage",
    "lint": "npx eslint . --fix"
  },
  "private": true,
